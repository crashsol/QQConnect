<Project Sdk="Microsoft.NET.Sdk" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>LDMING,stulzq</Authors>
    <Company />
    <Product>Unicorn.AspNetCore.Authentication.QQ</Product>
    <Description>适用于ASP.NET Core2.0+ 的 OAuth2 认证中间件，使Web应用程序支持用QQ快捷登录。</Description>
    <PackageProjectUrl>https://github.com/stulzq/QQConnect/tree/Unicorn</PackageProjectUrl>
    <PackageTags>QQConnect,QQLogin,security, aspnet core, authentication</PackageTags>
    <PackageLicenseUrl></PackageLicenseUrl>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>2.0.1.1</Version>
    <PackageId>Unicorn.AspNetCore.Authentication.QQ</PackageId>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Authentication.OAuth" Version="2.0.0" />
  </ItemGroup>

</Project>
